                        <option value="CE">CE</option>
                        <option value="ME">ME</option>
                        <option value="CE">EE</option>
                        <option value="BBA">BBA</option>
                        <option value="MBA">MBA</option>    
                      </select>
                    </div>

                    <br>

                    <br>
                    <br>


                    <div class="details personal">
                      <div class="fields">

                        <div class="input-field">
